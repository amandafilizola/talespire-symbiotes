{
  "manifestVersion": 1,
  "name": "Broadcast Images",
  "entryPoint": "/broadcast-image.html",
  "summary": "A quick way to broadcast images to all players.",
  "version": "0.0.1",
  "descriptionFilePath": "/README.md",
  "license": "MIT",
  "about": {
    "website": "https://github.com/benjaminsampica/talespire-symbiotes",
    "authors": [
      "Ben Sampica"
    ]
  },
  "api": {
    "version": "0.1",
    "interop": {
      "id": "6063e63e-bc68-45b2-985d-2205c6ddd8c3"
    },
    "subscriptions": {
      "sync": {
        "onSyncMessage": "handleSyncEvents"
      },
      "symbiote": {
        "onStateChangeEvent": "onStateChangeEventAsync"
      }
    }
  },
  "controls": {
    "navigation": true,
    "reload": true
  },
  "icons": {
    "64x64": "/64x64.png",
    "notification": "/notification.png"
  },
  "environment": {
    "webViewBackgroundColor": "#000000",
    "extras": [
      "fonts",
      "icons",
      "colorStyles"
    ],
    "capabilities": [
      "runInBackground"
    ]
  }
}